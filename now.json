{
    "name": "backend-booking",
    "version": 2,
    "builds": [{ "src": "src/index.js", "use": "@now/node-server" }],
    "routes": [{ "src": "/(.*)", "dest": "/src/index.js" }],
    "env": {
        "DB_USER": "@dbuser",
        "DB_PASSWORD": "@dbpassword",
        "DB_HOST": "@dbhost",
        "DB_NAME": "@dbname",
        "DEFAULT_ADMIN_PASSWORD": "@defaultadminpassword",
        "DEFAULT_USER_PASSWORD": "@defaultuserpassword",
        "AUTH_JWT_SECRET": "@authjwtsecret",
        "USER_API_KEY_TOKEN": "@userapikeytoken",
        "ADMIN_API_KEY_TOKEN": "@adminapikeytoken",
        "OWNER_API_KEY_TOKEN": "@ownerapikeytoken"
    }
}